<div id="title" class="title">
  <h1 class="title">Cuisines</h1>
  <br><br>
</div>

<div id="cuisine-list" class="cuisine-list">
  <% @cuisines.each do |cuisine| %>
    <p class="cuisineList lead"><%= link_to cuisine.name, cuisine_path(cuisine) %><br><%= cuisine.restaurants.count %> Restaurants</p>
  <% end %>
</div>
