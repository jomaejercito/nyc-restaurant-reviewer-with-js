<div id="title" class="title">
  <h1 class="editReview">Edit Your Review of <%= @review.restaurant.name %></h1>
  <br><br>
</div>

  <%= form_for :review, url: user_review_path, :method => 'patch' do |f| %>
    <%= render partial: 'form', locals: {f: f} %>
    <br><br><br>
    <%= f.submit "Update Review", class: "btn btn-outline-warning" %>
  <% end %>
